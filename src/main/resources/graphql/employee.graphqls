type Employee {
    id: ID!
    name: String!
    dateOfBirth: String!
    city: String
    salary: Float!
    gender: Gender!
    email: String!
}

enum Gender {
    M
    F
    X
}

type Query {
    employees: [Employee]!
    employee(id: ID!): Employee
}

type Mutation {
    addEmployee(name: String!, dateOfBirth: String!, city: String, salary: Float!, gender: Gender!, email: String!): Employee
}