type Query {
    employees: [Employee]
    employee(id: ID!): Employee
}

type Employee {
    id: ID!
    name: String!
    dateOfBirth: String!
    city: String!
    salary: Float!
    gender: String!
    email: String!
}

input CreateEmployeeInput {
    name: String!
    dateOfBirth: String!
    city: String!
    salary: Float!
    gender: String!
    email: String!
}

extend type Mutation {
    newEmployee(createEmployeeInput: CreateEmployeeInput!) : Employee!
}
